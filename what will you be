import random
print('HII THERE, THIS PROGRAM WILL TELL YOU WHAT YOU WILL BE IN THE FUTURE')
a=input("Please enter your name here\n")
jobs=['Engineer','Doctor','Youtuber','Buissnessman','journalist','labour','hacker']
r=random.choice(jobs)
b=f"Hii {a} according to this computer you will be a {r}"
print(b)
